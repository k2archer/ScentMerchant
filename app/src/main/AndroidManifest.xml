<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kwei.scentmerchant">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera.autofocus" /> <!-- 自动聚焦权限 -->


    <!-- cn.yipianfengye.android:zxing-library:2.2 低版本 SDK 需要权限 -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".StartActivity" />
        <activity android:name=".activity.ShopActivity" />
        <activity android:name=".ScanActivity" />
        <activity android:name=".activity.StickerDetailActivity" />
        <activity android:name=".activity.MyShopActivity" />
        <activity android:name=".activity.AdministratorActivity" />
        <activity android:name=".activity.StickerIncomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>